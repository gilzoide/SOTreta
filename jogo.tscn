script/source = "extends StreamPlayer

func _on_BGM_finished():
	set_loop(true)
	set_stream(preload(\"res://sons/musica.ogg\"))
	play()
	get_node(\"jogador1\").play()
	get_node(\"jogador2\").play()
"

